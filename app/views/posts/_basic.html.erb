<h1> Instagram </h1>
<%= link_to 'New Post', new_post_path %>
<%= link_to 'View My Posts',new_post_myposts_url %>
  <% @posts.each do |post| %>
  	<div>
      <p>Created At: <%= post.created_at %></p>
      <p>Updated At: <%= post.updated_at %></p>
      <% post.pictures.each do |pics| %>
      <%= image_tag(pics, size: "200x300") %>
     	<% end %>
  <% end %>
</div>
